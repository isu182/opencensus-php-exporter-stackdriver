{
    "name": "opencensus/opencensus-exporter-stackdriver",
      "description": "OpenCensus Stackdriver Exporter for PHP, tmp",
    "type": "library",
    "repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/hackerboydotcom/opencensus-php"
        }
    ],
    "require": {
        "php": ">=5.6",
        "opencensus/opencensus":"dev-patch-1",
        "google/cloud-trace": "~0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jeff Ching",
            "email": "chingor@google.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "OpenCensus\\Trace\\Exporter\\": "src/"
        }
    }
}
